<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">


    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints">

        <ImageButton
            android:id="@+id/paizhaodaoru"
            android:layout_width="164dp"
            android:layout_height="44dp"
            android:background="@drawable/photo_05_1_2_2"/>
        <ImageButton
            android:id="@+id/bendidaoru"
            android:layout_width="164dp"
            android:layout_height="44dp"
            android:background="@drawable/local_05_1_2_2"
            android:layout_below="@+id/paizhaodaoru"/>
        <ImageButton
            android:id="@+id/yunduandaoru"
            android:layout_width="164dp"
            android:layout_height="44dp"
            android:background="@drawable/cloud_05_1_2_2"
            android:layout_below="@id/bendidaoru"/>



    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>